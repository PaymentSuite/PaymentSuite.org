{% extends 'PaymentSuiteFrontBundle::layout.html.twig' %}

{% block meta_tags %}
	<meta name="description" content="">
	<meta name="keywords" content="">
	<meta name="robots" content="index,follow">
{% endblock %}

{% block body %}

	{% include 'PaymentSuiteFrontBundle:Commons:header.html.twig' %}

	{% set section_id = '_section1' | trans | lower | replace({' ' : '-'}) %}
	{% include 'PaymentSuiteFrontBundle:Commons:section.html.twig' with { 'id' : section_id, 'number' : 1, 'type' : 'main' } %}

	{% set section_id = '_section2' | trans | lower | replace({' ' : '-'}) %}
	{% include 'PaymentSuiteFrontBundle:Commons:section.html.twig' with { 'id' : section_id, 'number' : 2, 'type' : 'default' } %}

	{% set section_id = '_section3' | trans | lower | replace({' ' : '-'}) %}
	{% include 'PaymentSuiteFrontBundle:Commons:section.html.twig' with { 'id' : section_id, 'number' : 3, 'type' : 'default' } %}

	{% set section_id = '_section4' | trans | lower | replace({' ' : '-'}) %}
	{% include 'PaymentSuiteFrontBundle:Commons:section.html.twig' with { 'id' : section_id, 'number' : 4, 'type' : 'blocks' } %}
	
	{% include 'PaymentSuiteFrontBundle:Commons:footer.html.twig' %}

{% endblock %}
